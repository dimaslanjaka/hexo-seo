{
    "name": "hexo-seo",
    "version": "1.0.29",
    "description": "Automated Hexo Seo Optimizer",
    "main": "index.js",
    "files": [
        "dist",
        "src",
        "packages",
        "test.js",
        "*.md",
        "images",
        "source",
        "index.*"
    ],
    "scripts": {
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts,.json --fix",
        "ts:normal": "npx tsc -p tsconfig.json",
        "ts:build": "npx tsc -p tsconfig.build.json && npm run ts:defs && npm run ts:cjs && npm run ts:es",
        "ts:defs": "npx tsc -p tsconfig.build.json --declaration --outDir ./dist/types --emitDeclarationOnly",
        "ts:cjs": "npx tsc -p tsconfigs/cjs.tsconfig.json",
        "ts:es": "npx tsc -p tsconfigs/es.tsconfig.json",
        "build": "tsc && gulp",
        "prototypes": "npm i ./packages/js-prototypes && npm i git+https://github.com/dimaslanjaka/js-prototypes.git"
    },
    "repository": "dimaslanjaka/hexo-seo",
    "keywords": [
        "hexo",
        "search engine optimization",
        "auto",
        "seo"
    ],
    "engines": {
        "node": ">=8"
    },
    "author": {
        "email": "dimaslanjaka@gmail.com",
        "name": "Dimas Lanjaka",
        "url": "https://github.com/dimaslanjaka"
    },
    "license": "ISC",
    "dependencies": {
        "bluebird": "^3.7.2",
        "cheerio": "^1.0.0-rc.10",
        "clean-css": "^5.2.2",
        "file-type": "^16.5.3",
        "hexo": "^5.4.0",
        "hexo-log": "^3.0.0",
        "html-minifier-terser": "^6.0.2",
        "imagemin": "^6.1.0",
        "imagemin-giflossy": "^5.1.10",
        "imagemin-gifsicle": "^6.0.1",
        "imagemin-jpeg-recompress": "^6.0.0",
        "imagemin-jpegtran": "^6.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-optipng": "^6.0.0",
        "imagemin-pngquant": "^7.0.0",
        "imagemin-svgo": "^7.0.0",
        "imagemin-webp": "^5.0.0",
        "imagemin-zopfli": "^7.0.0",
        "jquery": "^3.6.0",
        "js-prototypes": "github:dimaslanjaka/js-prototypes",
        "jsdom": "^18.1.0",
        "lodash": "^4.17.21",
        "minimatch": "^3.0.4",
        "minimist": "^1.2.5",
        "moment": "^2.29.1",
        "node-cache": "^5.1.2",
        "node-html-parser": "^5.1.0",
        "node-libcurl": "^2.3.3",
        "object-assign": "^4.1.1",
        "parse5": "^6.0.1",
        "prompts": "^2.4.2",
        "sanitize-filename": "^1.6.3",
        "serve-static": "github:expressjs/serve-static",
        "terser": "^5.9.0",
        "ts-node": "^10.4.0",
        "underscore": "^1.13.1",
        "url-parse": "^1.5.3",
        "xmlbuilder2": "^3.0.2"
    },
    "devDependencies": {
        "@types/clean-css": "^4.2.5",
        "@types/hexo": "file:packages/@types/hexo",
        "@types/hexo-log": "file:packages/@types/hexo-log",
        "@types/jsdom": "^16.2.13",
        "@types/node": "^17.0.1",
        "@types/object-assign": "^4.0.30",
        "@types/url-parse": "^1.4.4",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "del": "^6.0.0",
        "dotenv": "^10.0.0",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "prettier": "^2.4.1",
        "prompt": "^1.2.0",
        "rimraf": "^3.0.2",
        "sinon": "^12.0.1",
        "typescript": "^4.5.4"
    },
    "peerDependencies": {
        "decompress": "git+https://github.com/kevva/decompress.git",
        "json-schema": "git+https://github.com/kriszyp/json-schema.git",
        "lodash.template": "git+https://github.com/azproduction/lodash-template.git",
        "semver-regex": ">=3.1.3",
        "trim-newlines": ">=3.0.1",
        "tunnel-agent": ">=0.6.0",
        "url-regex": "github:kevva/url-regex"
    }
}
