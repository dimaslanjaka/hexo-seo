{
  "name": "hexo-seo",
  "version": "1.1.5",
  "description": "Automated Seo Optimizer For Hexo",
  "main": "index.js",
  "files": [
    "dist",
    "src",
    "*.{md,json,js}",
    "images",
    "source",
    "LICENSE",
    "images"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts,.json --fix",
    "ts:normal": "npx tsc -p tsconfig.build.json",
    "ts:cjs": "npx tsc -p tsconfigs/cjs.tsconfig.json",
    "ts:es": "npx tsc -p tsconfigs/es.tsconfig.json",
    "build": "run-s build:** && gulp",
    "build:tsc": "npm run ts:normal",
    "build:eslint": "npx eslint --fix --quiet postinstall.js",
    "postbuild": "node packer.js && node unused.js",
    "clean": "gulp clean",
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml && curl https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.gitattributes > .gitattributes && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/changelog.js > changelog.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js && npx npm-check-updates -u -x chalk,hexo && npm install --cache tmp/npm",
    "postinstall": "node postinstall.js",
    "test:install": "cd test/demo && npm install"
  },
  "repository": "dimaslanjaka/hexo-seo",
  "keywords": [
    "hexo",
    "search engine optimization",
    "auto",
    "seo"
  ],
  "engines": {
    "node": ">=8",
    "hexo": ">=6"
  },
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://github.com/dimaslanjaka"
  },
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "^5.3.1",
    "deepmerge-ts": "^4.2.2",
    "file-type": "^18.0.0",
    "hexo": "^6.3.0",
    "hexo-is": "https://github.com/dimaslanjaka/hexo-is/tarball/master",
    "hexo-log": "^4.0.1",
    "html-minifier-terser": "^7.1.0",
    "jsdom": "^21.0.0",
    "minimatch": "^5.1.2",
    "minimist": "^1.2.7",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "node-cache": "^5.1.2",
    "node-html-parser": "^6.1.4",
    "node-libcurl": "^3.0.0",
    "nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
    "persistent-cache": "https://github.com/dimaslanjaka/persistent-cache/tarball/improve2",
    "sanitize-filename": "^1.6.3",
    "serve-static": "^1.15.0",
    "terser": "^5.16.1",
    "ts-node": "^10.9.1",
    "underscore": "^1.13.6",
    "url-parse": "^1.5.10",
    "xmlbuilder2": "^3.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/clean-css": "^4.2.6",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/object-assign": "^4.0.30",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/tarball/typescript",
    "depcheck": "^1.4.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prompt": "^1.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "glob-parent": ">=6.0.2",
    "node.extend": ">=2.0.2",
    "request": ">=2.88.2",
    "uuid": ">=9.0.0"
  }
}