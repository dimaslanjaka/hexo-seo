    "lint-and-fix": "eslint . --ext .ts,.json --fix",
    "postinstall": "npx tsc -p tsconfig.json",
    "ts:build": "npx tsc && npm run ts:defs && npm run ts:cjs && npm run ts:es",
    "ts:defs": "tsc -p tsconfig.build.json --declaration --outDir ./dist/types --emitDeclarationOnly",
    "ts:cjs": "tsc -p tsconfigs/cjs.tsconfig.json",
