{
  "name": "hexo-seo",
  "version": "1.0.35",
  "description": "Automated Hexo Seo Optimizer",
  "main": "index.js",
  "files": [
    "dist",
    "src",
    "packages",
    "test.js",
    "*.md",
    "images",
    "source",
    "index.*"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts,.json --fix",
    "ts:normal": "npx tsc -p tsconfig.build.json",
    "ts:cjs": "npx tsc -p tsconfigs/cjs.tsconfig.json",
    "ts:es": "npx tsc -p tsconfigs/es.tsconfig.json",
    "build": "npm run ts:normal && gulp",
    "clean": "gulp clean",
    "update": "npx npm-check-updates -u -x chalk,hexo"
  },
  "repository": "dimaslanjaka/hexo-seo",
  "keywords": [
    "hexo",
    "search engine optimization",
    "auto",
    "seo"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://github.com/dimaslanjaka"
  },
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "^5.3.1",
    "deepmerge-ts": "^4.2.2",
    "file-type": "^18.0.0",
    "hexo": "^5.4.0",
    "hexo-is": "file:packages/hexo-is",
    "hexo-log": "^4.0.0",
    "html-minifier-terser": "^7.0.0",
    "jsdom": "^20.0.1",
    "minimatch": "^5.1.0",
    "minimist": "^1.2.7",
