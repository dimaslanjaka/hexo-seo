    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "clean-css": "^5.2.2",
    "crypto-js": "^4.1.1",
    "file-type": "^16.5.3",
    "hexo-log": "^3.0.0",
    "html-minifier-terser": "^6.0.2",
    "js-prototypes": "file:packages/js-prototypes",
