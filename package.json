    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prompt": "^1.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
