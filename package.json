{
  "name": "hexo-seo",
  "version": "1.1.9",
  "description": "Automated Seo Optimizer For Hexo",
  "main": "index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src",
    "images",
    "source",
    "LICENSE",
    "readme.md",
    "index*.js"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts,.json --fix",
    "ts:normal": "tsc -p tsconfig.build.json",
    "ts:cjs": "tsc -p tsconfigs/cjs.tsconfig.json",
    "ts:es": "tsc -p tsconfigs/es.tsconfig.json",
    "build": "yarn ts:normal && gulp",
    "clean": "gulp clean",
    "update:project": "curl https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.gitattributes > .gitattributes && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/changelog.js > changelog.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npm-check-updates -u",
    "update": "run-s update:*",
    "post-install": "node postinstall.js --yarn --commit",
    "test:install": "cd test/demo && npm install",
    "pack": "node packer.js --yarn"
  },
  "repository": "dimaslanjaka/hexo-seo",
  "keywords": [
    "hexo",
    "search engine optimization",
    "auto",
    "seo"
  ],
  "engines": {
    "node": ">=8",
    "hexo": ">=6"
  },
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://github.com/dimaslanjaka"
  },
  "license": "ISC",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "^5.3.2",
    "deepmerge-ts": "^4.3.0",
    "file-type": "^18.2.1",
    "hexo": "^6.3.0",
    "hexo-is": "^1.0.5",
    "hexo-log": "^4.0.1",
    "html-minifier-terser": "^7.1.0",
    "jsdom": "^21.1.0",
    "minimatch": "^7.1.0",
    "minimist": "^1.2.8",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "node-cache": "^5.1.2",
    "node-html-parser": "^6.1.5",
    "node-libcurl": "^3.0.0",
    "nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
    "persistent-cache": "https://github.com/dimaslanjaka/persistent-cache/tarball/improve2",
    "rimraf": "^4.1.2",
    "sanitize-filename": "^1.6.3",
    "serve-static": "^1.15.0",
    "terser": "^5.16.5",
    "underscore": "^1.13.6",
    "url-parse": "^1.5.10",
    "xmlbuilder2": "^3.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/clean-css": "^4.2.6",
    "@types/hexo": "https://github.com/dimaslanjaka/nodejs-package-types/raw/hexo/release/types-hexo.tgz",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.1",
    "@types/object-assign": "^4.0.30",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/raw/through2/release/types-through2.tgz",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/tarball/private",
    "depcheck": "^1.4.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.7.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prompt": "^1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "optionalDependencies": {
    "glob-parent": ">=6.0.2",
    "node.extend": ">=2.0.2",
    "request": ">=2.88.2",
    "uuid": ">=9.0.0"
  },
  "packageManager": "yarn@3.4.1"
}
