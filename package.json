    "moment-timezone": "^0.5.38",
    "node-cache": "^5.1.2",
    "node-html-parser": "^6.1.1",
    "node-libcurl": "^2.3.4",
    "persistent-cache": "git+https://github.com/dimaslanjaka/persistent-cache#improve2",
    "sanitize-filename": "^1.6.3",
    "serve-static": "^1.15.0",
    "terser": "^5.15.1",
    "ts-node": "^10.9.1",
    "underscore": "^1.13.6",
    "url-parse": "^1.5.10",
    "xmlbuilder2": "^3.0.2"
  },
  "devDependencies": {
